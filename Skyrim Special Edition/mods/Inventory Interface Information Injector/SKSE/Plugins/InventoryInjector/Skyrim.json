{
  "$schema": "https://raw.githubusercontent.com/Exit-9B/InventoryInjector/main/docs/InventoryInjector.schema.json",
  "rules": [
    {
      "match": {
        "formType": "MiscItem",
        "keywords": "VendorItemFireword"
      },
      "assign": {
        "subType": "Firewood"
      }
    },
    {
      "match": {
        "formType": "MiscItem",
        "formId": {
          "anyOf": [
            "Skyrim.esm|1994F",
            "Skyrim.esm|59654",
            "Skyrim.esm|9DFBB",
            "Skyrim.esm|9F7A6"
          ]
        }
      },
      "assign": {
        "subType": "Gem"
      }
    },
    {
      "match": {
        "formType": "MiscItem",
        "formId": {
          "anyOf": [
            "Skyrim.esm|75868",
            "Skyrim.esm|F6767",
            "Skyrim.esm|AADB6",
            "Skyrim.esm|AADB7",
            "Skyrim.esm|4286C",
            "Dawnguard.esm|2993",
            "Dawnguard.esm|2994",
            "Dawnguard.esm|11CF7"
          ]
        }
      },
      "assign": {
        "subType": "Remains"
      }
    },
    {
      "match": {
        "formType": "Spell",
        "resistance": "FireResist"
      },
      "assign": {
        "iconLabel": "magic_fire",
        "iconColor": "#C73636"
      }
    },
    {
      "match": {
        "formType": "Spell",
        "resistance": "ElectricResist"
      },
      "assign": {
        "iconLabel": "magic_shock",
        "iconColor": "#EAAB00"
      }
    },
    {
      "match": {
        "formType": "Spell",
        "resistance": "FrostResist"
      },
      "assign": {
        "iconLabel": "magic_frost",
        "iconColor": "#1FFBFF"
      }
    },
    {
      "match": {
        "formType": "Scroll",
        "resistance": "FireResist"
      },
      "assign": {
        "iconColor": "#C73636"
      }
    },
    {
      "match": {
        "formType": "Scroll",
        "resistance": "ElectricResist"
      },
      "assign": {
        "iconColor": "#FFFF00"
      }
    },
    {
      "match": {
        "formType": "Scroll",
        "resistance": "FrostResist"
      },
      "assign": {
        "iconColor": "#1FFBFF"
      }
    },
    {
      "match": {
        "formType": "Armor",
        "weightClass": { "anyOf": [ "Jewelry", "Clothing", "Other" ] }
      },
      "assign": {
        "iconColor": "#EDDA87"
      }
    },
    {
      "match": {
        "formType": "Armor",
        "weightClass": "Light"
      },
      "assign": {
        "iconColor": "#756000"
      }
    },
    {
      "match": {
        "formType": "Armor",
        "weightClass": "Heavy"
      },
      "assign": {
        "iconColor": "#6B7585"
      }
    },
    {
      "match": {
        "formType": "Weapon"
      },
      "assign": {
        "iconColor": "#A4A5BF"
      }
    },
    {
      "match": {
        "formType": "Ammo"
      },
      "assign": {
        "iconColor": "#A89E8C"
      }
    },
    {
      "match": {
        "formType": "Potion",
        "subType": "Poison"
      },
      "assign": {
        "iconColor": "#AD00B3"
      }
    },
    {
      "match": {
        "formType": "Potion",
        "subType": { "anyOf": [ "Health", "HealRate", "HealRateMult" ] }
      },
      "assign": {
        "iconColor": "#DB2E73"
      }
    },
    {
      "match": {
        "formType": "Potion",
        "subType": { "anyOf": [ "Magicka", "MagickaRate", "MagickaRateMult" ] }
      },
      "assign": {
        "iconColor": "#2E9FDB"
      }
    },
    {
      "match": {
        "formType": "Potion",
        "subType": { "anyOf": [ "Stamina", "StaminaRate", "StaminaRateMult" ] }
      },
      "assign": {
        "iconColor": "#51DB2E"
      }
    },
    {
      "match": {
        "formType": "Potion",
        "subType": "FireResist"
      },
      "assign": {
        "iconColor": "#C73636"
      }
    },
    {
      "match": {
        "formType": "Potion",
        "subType": "ElectricResist"
      },
      "assign": {
        "iconColor": "#EAAB00"
      }
    },
    {
      "match": {
        "formType": "Potion",
