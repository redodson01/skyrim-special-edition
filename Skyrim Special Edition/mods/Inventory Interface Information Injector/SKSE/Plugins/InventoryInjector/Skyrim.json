      },
      "assign": {
        "iconColor": "#C0BAFF"
      }
    },
    {
      "match": {
        "formType": "SoulGem",
        "subType": "Common"
      },
      "assign": {
        "iconColor": "#ABA3FF"
      }
    },
    {
      "match": {
        "formType": "SoulGem",
        "subType": "Greater"
      },
      "assign": {
        "iconColor": "#948BFC"
      }
    },
    {
      "match": {
        "formType": "SoulGem",
        "subType": { "anyOf": [ "Grand", "Azura" ] }
