      "assign": {
        "iconColor": "#7569FF"
      }
    },
    {
      "match": {
        "formType": "MiscItem",
        "subType": "Gem"
      },
      "assign": {
        "iconColor": "#FFB0D1"
      }
    },
    {
      "match": {
        "formType": "MiscItem",
        "subType": "Hide"
      },
      "assign": {
        "iconColor": "#DBB36E"
      }
    },
    {
      "match": {
        "formType": "MiscItem",
        "subType": "Ingot"
      },
      "assign": {
        "iconColor": "#828282"
      }
    },
    {
      "match": {
        "formType": "MiscItem",
        "subType": "Firewood"
      },
      "assign": {
        "iconColor": "#A89E8C"
      }
    },
    {
      "match": {
        "formType": "MiscItem",
