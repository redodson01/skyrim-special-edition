<system xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="FSMP-Validator"
xsi:schemaLocation="FSMP-Validator I:/mods/utils/hdtSMP64.xsd">

  <bone name="NPC Head [Head]"/>
  
  <bone-default>
    <mass>0.01</mass>
    <inertia x="1" y="1" z="1"/>
    <linearDamping>0.5</linearDamping>
    <angularDamping>0.5</angularDamping>
  </bone-default>
  
  <bone name="JG Hair06 T1 L" />
  <bone name="JG Hair06 T1 R" />
  <bone name="JG Hair06 S1 L" />
  <bone name="JG Hair06 S2 L" />
  <bone name="JG Hair06 S1 R" />
  <bone name="JG Hair06 S2 R" />
  <bone name="JG Hair06 B1 L" />
  <bone name="JG Hair06 B2 L" />
  <bone name="JG Hair06 B1 R" />
  <bone name="JG Hair06 B2 R" />
  
  <bone-default>
    <mass>0.02</mass>
    <inertia x="2" y="1" z="2"/>
    <linearDamping>0</linearDamping>
    <angularDamping>0</angularDamping>
  </bone-default>
  
  <bone name="JG Hair06 Braid1 L" />
  <bone name="JG Hair06 Braid2 L" />
  <bone name="JG Hair06 Braid3 L" />
  <bone name="JG Hair06 Braid1 R" />
  <bone name="JG Hair06 Braid2 R" />
  <bone name="JG Hair06 Braid3 R" />

  <bone-default>
    <mass>0</mass>
  </bone-default>
  
  <generic-constraint-default name="base">
    <frameInA>
      <basis x="0" y="0" z="0" w="1"/>
      <origin x="0" y="-3.75" z="0"/>
    </frameInA>
    <useLinearReferenceFrameA>false</useLinearReferenceFrameA>
    <linearLowerLimit x="0" y="0" z="0"/>
    <linearUpperLimit x="0" y="0" z="0"/>
    <angularLowerLimit x="0" y="0" z="-0.3"/>
    <angularUpperLimit x="0.5" y="0" z="0.3"/>
    <angularStiffness x="1" y="1" z="1"/>
    <angularDamping x="3" y="1" z="3"/>
  </generic-constraint-default>
  
  <generic-constraint-default name="back_left" extends="base">
    <angularLowerLimit x="0" y="0" z="-0.5"/>
    <angularUpperLimit x="0.3" y="0" z="0.2"/>
  </generic-constraint-default>
  
  <generic-constraint-default name="back_right" extends="base">
    <angularLowerLimit x="0" y="0" z="-0.2"/>
    <angularUpperLimit x="0.3" y="0" z="0.5"/>
  </generic-constraint-default>
  
  <generic-constraint-default name="top" extends="base">
    <frameInA>
      <basis x="0" y="0" z="0" w="1"/>
      <origin x="0" y="-4" z="0"/>
    </frameInA>
    <angularStiffness x="5" y="1" z="5"/>
  </generic-constraint-default>
  
  <generic-constraint-default name="top_left" extends="top">
    <angularLowerLimit x="0" y="0" z="-1"/>
    <angularUpperLimit x="0.5" y="0" z="0"/>
  </generic-constraint-default>
  
  <generic-constraint-default name="top_right" extends="top">
    <angularLowerLimit x="0" y="0" z="0"/>
    <angularUpperLimit x="0.5" y="0" z="1"/>
  </generic-constraint-default>

  <generic-constraint-default name="braid">
    <frameInA>
      <basis w="1" x="0" y="0" z="0"/>
      <origin x="0" y="-2.5" z="0"/>
    </frameInA>
    <useLinearReferenceFrameA>false</useLinearReferenceFrameA>
    <linearLowerLimit x="0" y="0" z="0"/>
    <linearUpperLimit x="0" y="0" z="0"/>
    <angularLowerLimit x="0" y="0" z="-1"/>
    <angularUpperLimit x="1" y="0" z="1"/>
    <angularStiffness x="30" y="1" z="30"/>
    <angularDamping x="10" y="0" z="10"/>
  </generic-constraint-default>
  
  <generic-constraint bodyA="JG Hair06 T1 L" bodyB="NPC Head [Head]" template="top_left"/>
  <generic-constraint bodyA="JG Hair06 T1 R" bodyB="NPC Head [Head]" template="top_right"/>
  
  <constraint-group>
    <generic-constraint bodyA="JG Hair06 S1 L" bodyB="NPC Head [Head]" template="base"/>
    <generic-constraint bodyA="JG Hair06 S2 L" bodyB="JG Hair06 S1 L" template="base"/>
  </constraint-group>
  <constraint-group>
    <generic-constraint bodyA="JG Hair06 S1 R" bodyB="NPC Head [Head]" template="base"/>
    <generic-constraint bodyA="JG Hair06 S2 R" bodyB="JG Hair06 S1 R" template="base"/>
  </constraint-group>
  <constraint-group>
    <generic-constraint bodyA="JG Hair06 B1 L" bodyB="NPC Head [Head]" template="back_left"/>
    <generic-constraint bodyA="JG Hair06 B2 L" bodyB="JG Hair06 B1 L" template="back_left"/>
  </constraint-group>
  <constraint-group>
    <generic-constraint bodyA="JG Hair06 B1 R" bodyB="NPC Head [Head]" template="back_right"/>
    <generic-constraint bodyA="JG Hair06 B2 R" bodyB="JG Hair06 B1 R" template="back_right"/>
  </constraint-group>
  
  <constraint-group>
    <generic-constraint bodyA="JG Hair06 Braid1 L" bodyB="NPC Head [Head]" template="braid"/>
    <generic-constraint bodyA="JG Hair06 Braid2 L" bodyB="JG Hair06 Braid1 L" template="braid"/>
    <generic-constraint bodyA="JG Hair06 Braid3 L" bodyB="JG Hair06 Braid2 L" template="braid"/>
  </constraint-group>
  <constraint-group>
    <generic-constraint bodyA="JG Hair06 Braid1 R" bodyB="NPC Head [Head]" template="braid"/>
    <generic-constraint bodyA="JG Hair06 Braid2 R" bodyB="JG Hair06 Braid1 R" template="braid"/>
    <generic-constraint bodyA="JG Hair06 Braid3 R" bodyB="JG Hair06 Braid2 R" template="braid"/>
  </constraint-group>
	
</system>