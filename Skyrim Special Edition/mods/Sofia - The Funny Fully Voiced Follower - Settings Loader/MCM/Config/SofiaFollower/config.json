                    "id": "sRemoveAllSpells:Combat",
                    "type": "text",
                    "text": "$RemoveAllSpells",
                    "help": "$RemoveAllSpellsCombat_Help",
                    "valueOptions": {
                        "sourceType": "ModSettingString",
                        "defaultValue": ""
                    },
                    "action": {
                        "type": "CallFunction",
