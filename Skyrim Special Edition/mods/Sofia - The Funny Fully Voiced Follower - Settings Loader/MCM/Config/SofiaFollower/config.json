                    "id": "bEnableSandboxing:SofiaConfig",
                    "type": "toggle",
                    "text": "$EnableSandboxing",
                    "help": "$EnableSandboxingSofiaConfig_Help",
                    "valueOptions": {
                        "sourceType": "ModSettingBool",
                        "defaultValue": true
                    }
                },
                {
                    "id": "bIsEssential:SofiaConfig",
                    "type": "toggle",
                    "text": "$IsEssential",
                    "help": "$IsEssentialSofiaConfig_Help",
