{
    "modName": "SofiaFollower",
    "displayName": "$SofiaFollower",
    "minMcmVersion": 9,
    "pluginRequirements": [],
    "cursorFillMode": "topToBottom",
    "content": [],
    "customContent": {},
    "pages": [
        {
            "pageDisplayName": "$SofiaSettings_Page",
            "cursorFillMode": "topToBottom",
            "content": [
                {
                    "type": "header",
                    "text": "$SofiaConfig_Header"
                },
                {
                    "id": "bEnableCatchUp:SofiaConfig",
                    "type": "toggle",
                    "text": "$EnableCatchUp",
                    "help": "$EnableCatchUpSofiaConfig_Help",
                    "valueOptions": {
                        "sourceType": "ModSettingBool",
                        "defaultValue": true
                    }
                },
                {
                    "id": "iCatchUpDistance:SofiaConfig",
                    "type": "slider",
                    "text": "$CatchUpDistance",
                    "help": "$CatchUpDistanceSofiaConfig_Help",
                    "valueOptions": {
                        "min": 200,
                        "max": 10000,
                        "step": 1,
                        "defaultValue": 3000,
                        "sourceType": "ModSettingInt",
                        "formatString": "{0} units"
                    }
                },
                {
                    "type": "empty"
                },
                {
                    "id": "bDisableCommentsDialogue:SofiaConfig",
                    "type": "toggle",
                    "text": "$DisableCommentsDialogue",
                    "help": "$DisableCommentsDialogueSofiaConfig_Help",
                    "valueOptions": {
                        "sourceType": "ModSettingBool",
                        "defaultValue": false
                    }
                },
                {
                    "id": "fCommentsFrequency:SofiaConfig",
                    "type": "slider",
                    "text": "$CommentsFrequency",
                    "help": "$CommentsFrequencySofiaConfig_Help",
                    "valueOptions": {
                        "min": 0.1,
                        "max": 24.0,
                        "step": 0.1,
                        "defaultValue": 1.0,
                        "sourceType": "ModSettingFloat",
                        "formatString": "{1} hours"
                    }
