                    "id": "iCombatStyle:Combat",
                    "type": "enum",
                    "text": "$CombatStyle",
                    "help": "",
                    "valueOptions": {
                        "options": [
                            "Spell Sword (Default)",
                            "One Handed",
                            "Two Handed",
                            "Magic",
                            "Ranged",
                            "Dual Wield"
                        ],
                        "sourceType": "ModSettingInt",
                        "defaultValue": 0
                    }
                },
                {
                    "type": "empty"
                },
