<system xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="FSMP-Validator"
xsi:schemaLocation="FSMP-Validator I:/mods/utils/hdtSMP64.xsd">

  <bone name="NPC Head [Head]"/>
  
  <bone-default>
    <mass>0.01</mass>
    <inertia x="1" y="1" z="1"/>
    <linearDamping>0.5</linearDamping>
    <angularDamping>0.5</angularDamping>
  </bone-default>
  
  <bone name="JG Hair08 B1 L" />
  <bone name="JG Hair08 B1 R" />
  <bone name="JG Hair08 F1 L" />
  <bone name="JG Hair08 F1 R" />
  
  <bone name="JG Hair08 Tail1" />

  <bone-default>
    <mass>0.02</mass>
    <inertia x="2" y="1" z="2"/>
    <linearDamping>0</linearDamping>
    <angularDamping>0</angularDamping>
  </bone-default>
  
  <bone name="JG Hair08 Braid01 1 L" />
  <bone name="JG Hair08 Braid01 2 L" />
  <bone name="JG Hair08 Braid01 1 R" />
  <bone name="JG Hair08 Braid01 2 R" />
  
  <bone name="JG Hair08 Braid02 1 L" />
  <bone name="JG Hair08 Braid02 2 L" />
  <bone name="JG Hair08 Braid02 1 R" />
  <bone name="JG Hair08 Braid02 2 R" />
  
  <bone name="JG Hair08 Braid03 1 L" />
  <bone name="JG Hair08 Braid03 2 L" />
  <bone name="JG Hair08 Braid03 1 R" />
  <bone name="JG Hair08 Braid03 2 R" />

  <bone-default>
    <mass>0</mass>
  </bone-default>
  
  <generic-constraint-default name="base">
    <frameInA>
      <basis x="0" y="0" z="0" w="1"/>
      <origin x="0" y="-5" z="0"/>
    </frameInA>
    <useLinearReferenceFrameA>false</useLinearReferenceFrameA>
    <linearLowerLimit x="0" y="0" z="0"/>
    <linearUpperLimit x="0" y="0" z="0"/>
    <angularLowerLimit x="0" y="0" z="-1"/>
    <angularUpperLimit x="1" y="0" z="1"/>
    <angularStiffness x="1" y="1" z="1"/>
    <angularDamping x="3" y="1" z="3"/>
  </generic-constraint-default>
  
  <generic-constraint-default name="back_left" extends="base">
    <angularLowerLimit x="0" y="0" z="-1"/>
    <angularUpperLimit x="0.5" y="0" z="0.2"/>
  </generic-constraint-default>
  
  <generic-constraint-default name="back_right" extends="back_left">
    <angularLowerLimit x="0" y="0" z="-0.2"/>
    <angularUpperLimit x="0.5" y="0" z="1"/>
  </generic-constraint-default>

  <generic-constraint-default name="braid">
    <frameInA>
      <basis w="1" x="0" y="0" z="0"/>
      <origin x="0" y="-3" z="0"/>
    </frameInA>
    <useLinearReferenceFrameA>false</useLinearReferenceFrameA>
    <linearLowerLimit x="0" y="0" z="0"/>
    <linearUpperLimit x="0" y="0" z="0"/>
    <angularLowerLimit x="0" y="0" z="-1"/>
    <angularUpperLimit x="1" y="0" z="1"/>
    <angularStiffness x="30" y="1" z="30"/>
    <angularDamping x="10" y="0" z="10"/>
  </generic-constraint-default>

  <generic-constraint-default name="tail" extends="braid">
    <frameInA>
      <basis w="1" x="0" y="0" z="0"/>
      <origin x="0" y="-4" z="0"/>
    </frameInA>
    <angularLowerLimit x="0" y="0" z="-1"/>
    <angularUpperLimit x="1" y="0" z="1"/>
    <angularStiffness x="20" y="1" z="20"/>
    <angularDamping x="3" y="0" z="3"/>
  </generic-constraint-default>
  
  <generic-constraint bodyA="JG Hair08 F1 L" bodyB="NPC Head [Head]" template="base" />
  <generic-constraint bodyA="JG Hair08 F1 R" bodyB="NPC Head [Head]" template="base" />
  <generic-constraint bodyA="JG Hair08 B1 L" bodyB="NPC Head [Head]" template="back_left" />
  <generic-constraint bodyA="JG Hair08 B1 R" bodyB="NPC Head [Head]" template="back_right" />
  
  <constraint-group>
    <generic-constraint bodyA="JG Hair08 Braid01 1 L" bodyB="NPC Head [Head]" template="braid" />
    <generic-constraint bodyA="JG Hair08 Braid01 2 L" bodyB="JG Hair08 Braid01 1 L" template="braid" />
  </constraint-group>
  <constraint-group>
    <generic-constraint bodyA="JG Hair08 Braid01 1 R" bodyB="NPC Head [Head]" template="braid" />
    <generic-constraint bodyA="JG Hair08 Braid01 2 R" bodyB="JG Hair08 Braid01 1 R" template="braid" />
  </constraint-group>
  <constraint-group>
    <generic-constraint bodyA="JG Hair08 Braid02 1 L" bodyB="NPC Head [Head]" template="braid" />
    <generic-constraint bodyA="JG Hair08 Braid02 2 L" bodyB="JG Hair08 Braid02 1 L" template="braid" />
  </constraint-group>
  <constraint-group>
    <generic-constraint bodyA="JG Hair08 Braid02 1 R" bodyB="NPC Head [Head]" template="braid" />
    <generic-constraint bodyA="JG Hair08 Braid02 2 R" bodyB="JG Hair08 Braid02 1 R" template="braid" />
  </constraint-group>
  <constraint-group>
    <generic-constraint bodyA="JG Hair08 Braid03 1 L" bodyB="NPC Head [Head]" template="braid" />
    <generic-constraint bodyA="JG Hair08 Braid03 2 L" bodyB="JG Hair08 Braid03 1 L" template="braid" />
  </constraint-group>
  <constraint-group>
    <generic-constraint bodyA="JG Hair08 Braid03 1 R" bodyB="NPC Head [Head]" template="braid" />
    <generic-constraint bodyA="JG Hair08 Braid03 2 R" bodyB="JG Hair08 Braid03 1 R" template="braid" />
  </constraint-group>

  <generic-constraint bodyA="JG Hair08 Tail1" bodyB="NPC Head [Head]" template="tail"/>
	
</system>