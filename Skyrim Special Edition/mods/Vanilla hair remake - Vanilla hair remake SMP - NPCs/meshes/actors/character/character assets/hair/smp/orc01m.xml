<system xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="FSMP-Validator"
xsi:schemaLocation="FSMP-Validator I:/mods/utils/hdtSMP64.xsd">

  <bone name="NPC Head [Head]"/>

  <bone-default>
    <mass>0.01</mass>
    <inertia x="1" y="1" z="1"/>
    <linearDamping>0.5</linearDamping>
    <angularDamping>0.5</angularDamping>
  </bone-default>
  
  <bone name="JG Orc01 Tail1 L" />
  <bone name="JG Orc01 Tail2 L" />
  <bone name="JG Orc01 Tail1 R" />
  <bone name="JG Orc01 Tail2 R" />
  
  <bone-default>
    <mass>0.01</mass>
    <inertia x="1" y="1" z="1"/>
    <linearDamping>0.8</linearDamping>
    <angularDamping>0.8</angularDamping>
  </bone-default>
  
  <bone name="JG Orc01 S1 L" />
  <bone name="JG Orc01 S2 L" />
  <bone name="JG Orc01 S1 R" />
  <bone name="JG Orc01 S2 R" />
  
  <bone-default>
    <mass>0.01</mass>
    <inertia x="1" y="1" z="1"/>
    <linearDamping>0.5</linearDamping>
    <angularDamping>0.5</angularDamping>
  </bone-default>
  
  <bone name="JG Orc01 B1" />
  <bone name="JG Orc01 B2" />

  <bone-default>
    <mass>0</mass>
  </bone-default>

  <generic-constraint-default name="tail">
    <frameInA>
      <basis x="0" y="0" z="0" w="1"/>
      <origin x="0" y="-3.5" z="0"/>
    </frameInA>
    <useLinearReferenceFrameA>false</useLinearReferenceFrameA>
    <linearLowerLimit x="0" y="0" z="0"/>
    <linearUpperLimit x="0" y="0" z="0"/>
    <angularStiffness x="3" y="1" z="3"/>
    <angularDamping x="2" y="1" z="2"/>
  </generic-constraint-default>

  <generic-constraint-default name="left" extends="tail">
    <angularLowerLimit x="0" y="0" z="-1.0"/>
    <angularUpperLimit x="1" y="0" z="0.5"/>
  </generic-constraint-default>
  
  <generic-constraint-default name="right" extends="tail">
    <angularLowerLimit x="0" y="0" z="-0.5"/>
    <angularUpperLimit x="1" y="0" z="1.0"/>
  </generic-constraint-default>
  
  <generic-constraint-default name="back">
    <frameInA>
      <basis x="0" y="0" z="0" w="1"/>
      <origin x="0" y="-3.25" z="0"/>
    </frameInA>
    <useLinearReferenceFrameA>false</useLinearReferenceFrameA>
    <linearLowerLimit x="0" y="0" z="0"/>
    <linearUpperLimit x="0" y="0" z="0"/>
    <angularStiffness x="1" y="1" z="1"/>
    <angularLowerLimit x="0" y="0" z="-0.5"/>
    <angularUpperLimit x="0.5" y="0" z="0.5"/>
    <angularDamping x="10" y="1" z="10"/>
  </generic-constraint-default>
  
  <generic-constraint-default name="side" extends="back">
    <frameInA>
      <basis x="0" y="0" z="0" w="1"/>
      <origin x="0" y="-2.5" z="0"/>
    </frameInA>
    <angularLowerLimit x="0" y="0" z="-0.5"/>
    <angularUpperLimit x="1" y="0" z="0.5"/>
    <angularDamping x="5" y="1" z="5"/>
  </generic-constraint-default>

  <constraint-group>
    <generic-constraint bodyA="JG Orc01 Tail1 L" bodyB="NPC Head [Head]" template="left"/>
    <generic-constraint bodyA="JG Orc01 Tail2 L" bodyB="JG Orc01 Tail1 L" template="left"/>
  </constraint-group>
  <constraint-group>
    <generic-constraint bodyA="JG Orc01 Tail1 R" bodyB="NPC Head [Head]" template="right"/>
    <generic-constraint bodyA="JG Orc01 Tail2 R" bodyB="JG Orc01 Tail1 R" template="right"/>
  </constraint-group>

  <constraint-group>
    <generic-constraint bodyA="JG Orc01 S1 L" bodyB="NPC Head [Head]" template="side" />
    <generic-constraint bodyA="JG Orc01 S2 L" bodyB="JG Orc01 S1 L" template="side" />
  </constraint-group>
  <constraint-group>
    <generic-constraint bodyA="JG Orc01 S1 R" bodyB="NPC Head [Head]" template="side" />
    <generic-constraint bodyA="JG Orc01 S2 R" bodyB="JG Orc01 S1 R" template="side" />
  </constraint-group>
  <constraint-group>
    <generic-constraint bodyA="JG Orc01 B1" bodyB="NPC Head [Head]" template="back" />
    <generic-constraint bodyA="JG Orc01 B2" bodyB="JG Orc01 B1" template="back" />
  </constraint-group>
	
</system>